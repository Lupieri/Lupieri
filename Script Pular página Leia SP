setInterval(() => {
    let botoes = document.querySelectorAll('.sc-fncAUw.jctfjC'); 
    let botaoDireita = botoes[botoes.length - 1]; // Pega o último botão (avançar)
    
    if (botaoDireita) {
        botaoDireita.click();
        console.log("Página avançada!");
    } else {
        console.log("Botão não encontrado!");
    }
}, 300000); // 5 minutos (300.000 milissegundos)
